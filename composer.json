{
  "name": "actinity/actinite",
  "type": "package",
  "description": "Actinite CMS",
  "require": {
    "php": "^8.1",
    "ext-fileinfo": "*",
    "ext-json": "*",
    "wa72/htmlpagedom": "^3.0"
  },
  "require-dev": {
    "orchestra/testbench": "^7.22",
    "phpunit/phpunit": "^9.6"
  },
  "autoload": {
    "psr-4": {
      "Actinity\\Actinite\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Actinity\\Actinite\\ActiniteServiceProvider"
      ]
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
